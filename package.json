{
  "name": "hlf-tumblr-log-development",
  "version": "0.1.1",
  "author": "Peng Wang <peng@pengxwang.com>",
  "description": "A light Tumblr blog theme with quality UI.",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-autoprefixer": "^0.8.0",
    "grunt-bump": "0.0.14",
    "grunt-contrib-clean": "^0.5.0",
    "grunt-contrib-coffee": "^0.10.1",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-contrib-sass": "^0.7.3",
    "grunt-contrib-watch": "^0.6.1",
    "matchdep": "^0.3.0"
  },
  "dependencies": {
    "hlf-css": "^0.2.0"
  },
  "scripts": {
    "postinstall": "npm run hlf-css:dist && npm run hlf-css:lib",
    "hlf-css:dist": "cp node_modules/hlf-css/dist/scss/imports/* lib/hlf-css",
    "hlf-css:lib": "cp node_modules/hlf-css/lib/modified/{_{fontawesome,normalize},html5-boilerplate/_{print,helpers}}.scss lib/hlf-css"
  }
}
